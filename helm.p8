pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- flyin around

-- constants
cam_offset = 64
player = {}
turn_speed = 0.05
tau = 6.28318530718

-- classes
heavenly_body = {}
function heavenly_body.new()
  body = {}
  setmetatable(body, { __index = heavenly_body })
  body.x = 0
  body.y = 0
  body.xvel = 0
  body.yvel = 0
  body.angle = 0
  body.sprite = 0
  body.size = 0
  body.accelerator = true
  body.acceleration = 0.2
  return body
end

function heavenly_body:draw()
  local r=flr(self.angle*20)/20
  local s=sin(r)
  local c=cos(r)
  local b=s*s+c*c
  local size = (self.size * 8) / 2
  local w = sqrt(size^2*2)
  local sprite_x, sprite_y = spritesheet_coords(self.sprite)
  for y=-w,w do
    for x=-w,w do
      local ox=( s*y+c*x)/b+size
      local oy=(-s*x+c*y)/b+size
      local col=sget(ox + sprite_x, oy + sprite_y)
      if col>0 then
        pset(self.x+x,self.y+y,col)
      end
    end
  end
end

function heavenly_body:update()
  local r=flr(self.angle*20)/20
  -- reduce velocity
  self.xvel *= 0.95
  self.yvel *= 0.95
  -- current speed
  local speed = sqrt(self.xvel^2 + self.yvel^2) 
  if self.accelerator then
    self.xvel += sin(r) * self.acceleration
    self.yvel -= cos(r) * self.acceleration
  end

  self.x = flr(self.x + self.xvel)
  self.y = flr(self.y + self.yvel)
  printh('angle: '..self.angle..' rotate: '..r..' x: '..self.x..' y: '..self.y)
end

function spritesheet_coords(sprite_num)
  -- sprite 1 is x8, 15 is 120, 16 is 0, etc
  local x = flr(sprite_num % 16) * 8
  -- sprite 16 is y8, 32 is 16, 40 is 24, etc
  local y = flr(sprite_num / 16) * 8
  return x, y
end

function _init()
  player = heavenly_body.new()
  player.sprite = 1
  player.size = 2
end

function _update()
  camera(player.x - cam_offset, player.y - cam_offset)

  if btn(0) then
    player.angle += turn_speed
    if (player.angle < 0) player.angle += 1
  end
  if btn(1) then
    player.angle -= turn_speed
    if (player.angle >= 1) player.angle -= 1
  end
  if btn(2) then
    player.accelerator = true
  else
    player.accelerator = false
  end

  player:update()
end

function _draw()
  cls()
  draw_planet()
  player:draw()
end

function draw_planet()
  local planet_diameter = 50
  circfill(0, 0, planet_diameter, 12) 
end



